    {
    flags = 0;
    buttons =
        {
        CBA_BUTTON {id=EAknSoftkeyOptions; 	txt=qtn_setting_option;},
        CBA_BUTTON {id=EListboxCmdBack; 	txt=qtn_setting_back;},
        CBA_BUTTON {id=EListboxCmdChange; 	txt=qtn_setting_change;}
        };
    }
